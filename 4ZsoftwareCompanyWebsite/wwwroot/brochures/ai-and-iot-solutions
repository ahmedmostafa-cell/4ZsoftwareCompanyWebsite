  <script type="text/javascript">

    $(document).ready(function(){
      $('#choosefile').on('change',function(){
          var fileName = $(this).val().split('\\').pop();
          $("#noFile").text(fileName)
      })
    })

    $("form#submitform").submit(function (e) {

      e.preventDefault();
      console.log(e);
      var formData = new FormData(this);
      console.log(formData);

      var e = $("#fname").val(),
        a = $("#email").val(),
        f = $("#othercontact").val(),
        c = $("#country").val(),
        r = $("#mobile").val(),
        l = $("#content").val();
       af = $("#choosefile").val();

      if (r.length < 10 || r.length > 15) {
        $("#error").html("Phone number should be 10 to 15 digits."), $("#errorMsg").show(), $("#mobile").addClass("alert-danger"), !1
      }


      return "" == e ? ($("#error").html("First Name is required."), $("#errorMsg").show(), $("#fname").addClass("alert-danger"), !1) : "" == a ? ($("#error").html("Email is required."), $("#errorMsg").show(), $("#email").addClass("alert-danger"), !1) : validateEmail(a) ? "" == r ? ($("#error").html("Phone number should be valid."), $("#errorMsg").show(), $("#mobile").addClass("alert-danger"), !1) : isNaN(parseInt(r)) ? ($("#error").html("Phone number should be number."), $("#errorMsg").show(), $("#mobile").addClass("alert-danger"), !1) : "" == l ? ($("#error").html("Mesage is required."), $("#errorMsg").show(), $("#content").addClass("alert-danger"), !1) : ($(".loader").show(),
        $.ajax({
          url: 'sendmobile-appmail',
          type: 'POST',
          dataType: 'json',
          data: formData,
          success: function (e) {
            "success" == e.status ? window.location = "https://www.aryausa.com/thanks.php" : (alert(e.msg), $(".loader").hide()), $("#noFile").text("No file chosen..."), closeError()
          },
          cache: false,
          contentType: false,
          processData: false
        }), !1) : ($("#error").html("Invalid email."), $("#errorMsg").show(), $("#email").addClass("alert-danger"), !1)

    });

    function closeError() {
      $("#error").html(" "), $("#errorMsg").hide(), $("#fname").removeClass("alert-danger"), $("#email").removeClass("alert-danger"), $("#mobile").removeClass("alert-danger"), $("#content").removeClass("alert-danger");
    }

    function validateEmail(e) {
      return /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/.test(e)
    }

  </script>
